{
	"name": "bmack/site-importer",
	"description": "Imports records from a Yaml file structure into the TYPO3 database",
	"license": "GPL-2.0+",
	"type": "typo3-cms-extension",
	"authors": [
		{
			"name": "Benni Mack",
			"email": "benni@typo3.org"
		}
	],
	"autoload": {
		"psr-4": {
			"Bmack\\SiteImporter\\": "src/"
		}
	},
	"require": {
		"php": "^8.1",
		"symfony/yaml": "^6 || ^7",
		"typo3/cms-core": "^12",
		"ext-json": "*"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "site_importer"
		}
	}
}
